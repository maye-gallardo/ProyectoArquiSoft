{
  "name": "generador-de-boletas",
  "version": "1.0.1",
  "author": "Mauricio Quezada Bustillo, Maria De Los Angeles Gallardo",
  "license": "ISC",
  "nyc": {
    "all": true,
    "include": [
      "*.js"
    ]
  },
  "scripts": {
    "coverage": "nyc mocha --require babel-register",
    "test": "mocha --require babel-register",
    "start": "nodemon server.js --exec babel-node --presets env",
    "release": "npm run clean && npm run build && npm run serve",
    "clean": "rm -rf dist && mkdir dist",
    "build": "babel . -s -D -d dist --presets env --ignore node_modules",
    "serve": "node dist/server.js"
  },
  "dependencies": {
    "chai": "^4.2.0",
    "express": "^4.16.4",
    "mocha": "^5.2.0",
    "mongoose": "^5.5.2"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-plugin-istanbul": "^5.1.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "nodemon": "^1.18.11",
    "nyc": "^13.2.0"
  },
  "description": "Generador de boleta de empleados",
  "main": "CalculadorComision.js",
  "directories": {
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/maye-gallardo/ProyectoArquiSoft.git"
  },
